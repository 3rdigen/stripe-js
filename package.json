{
  "name": "@stripe/stripe-js",
  "version": "1.0.0-beta.1",
  "description": "Stripe.js loading utility",
  "main": "dist/stripe.js",
  "module": "dist/stripe.esm.js",
  "jsnext:main": "dist/stripe.esm.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "build": "yarn lint && yarn rollup -c",

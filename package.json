    "prettier-list-different": "prettier './**/*.js' './**/*.css' './**/*.md' --list-different",
    "prepublish": "yarn run clean && yarn run build"
  },
  "keywords": [
    "Stripe",
    "Stripe.js",
    "Elements"
  ],
  "author": "Stripe (https://www.stripe.com)",
